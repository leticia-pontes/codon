// Exemplo de struct generico - Pair<T,U>

struct Pair<T, U> {
    first: T;
    second: U;
}

procedure main() {
    // Pair de int e string
    var Pair<int, string> p1;
    p1 = new Pair<int, string>();
    p1.first = 100;
    p1.second = "points";
    print("Pair<int,string>:", p1.first, p1.second);
    
    // Pair de string e int
    var Pair<string, int> p2;
    p2 = new Pair<string, int>();
    p2.first = "age";
    p2.second = 25;
    print("Pair<string,int>:", p2.first, p2.second);
}
