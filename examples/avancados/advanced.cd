function processar_dominios(tamanho: int): int {
    var int resultado;
    resultado = tamanho * 2;

    var int i;
    for (i = 1; i < 10; i = i + 1) {
        if (i >= 5) {
            print("Processando");
        }
    }

    return resultado;
}

function main(): int {
    var int resultado;
    resultado = processar_dominios(11);
    print("Tamanho:");
    print(resultado);
    
    return 0;
}