    print(1);  // Deve imprimir 1
}

if (x > 10 && y > 0) {
    print(99);  // N찾o deve imprimir
}

// Teste OR (||)
if (x > 10 || y > 5) {
    print(2);  // Deve imprimir 2
}

if (x > 10 || y < 5) {
    print(99);  // N찾o deve imprimir
}

// Teste de short-circuit
