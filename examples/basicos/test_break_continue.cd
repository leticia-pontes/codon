/" Teste de break e continue "/

// Teste break
var i = 0;
while (i < 10) {
    if (i == 5) {
        break;  // Para no 5
    }
    print(i);
    i += 1;
}

print(99);  // Separador

// Teste continue
var j = 0;
while (j < 5) {
    j += 1;
    if (j == 3) {
        continue;  // Pula o 3
    }
    print(j);
}

print(99);  // Separador

// Teste break em for
for (var k = 0; k < 10; k += 1) {
    if (k == 7) {
        break;
    }
    print(k);
}